    "url": "git+https://github.com/ubreu/homebridge-ibricks.git"
  },
  "bugs": {
    "url": "https://github.com/ubreu/homebridge-ibricks/issues"
  },
  "engines": {
    "node": "^18.20.4 || ^20.11.0 || ^22.5.1",
    "homebridge": "^1.8.0 || ^2.0.0-beta.0"
